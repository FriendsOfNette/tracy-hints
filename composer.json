{
  "name": "contributte/tracy",
  "description": "Tuned Tracy Bars/Panels/BlueScreens for easy-developing",
  "keywords": ["nette", "tracy", "profile", "bluescreen", "develop"],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/tracy",
  "authors": [
    {
      "name": "Milan Felix Sulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">= 5.6",
    "tracy/tracy": "~2.4.10"
  },
  "require-dev": {
    "ninjify/qa": "~0.4.0",
    "ninjify/nunjuck": "~0.2.0",
    "nette/di": "~2.4.10"
  },
  "suggest": {
    "nette/robot-loader": "to use AutoloadExtension[CompilerExtension]",
    "nette/reflection": "to use AutoloadExtension[CompilerExtension]"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Tracy\\": "src"
    }
  },
  "scripts": {
    "qa": [
      "linter src tests",
      "codesniffer src tests"
    ],
    "tester": [
      "tester -s -p php --colors 1 -C tests/cases"
    ],
    "tester-coverage": [
      "tester -s -p php --colors 1 -C -d extension=xdebug.so --coverage ./coverage.xml --coverage-src ./src tests/cases"
    ]
  }
}
